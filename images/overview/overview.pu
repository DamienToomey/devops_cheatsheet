skinparam rectangle {
  BackgroundColor<<working_directory>> #9fc2d5
	FontColor<<working_directory>> #214754
	BackgroundColor<<stage>> #d8acc7
	FontColor<<stage>> #653157
	BackgroundColor<<repository>> #acd6b2
	FontColor<<repository>> #2d5d2d
  FontStyle Bold
}

skinparam cloud {
	BackgroundColor #f7f7f7
	FontStyle Bold
	FontColor #000000
	'FontColor #636363
}

rectangle "Working directory" as working_directory <<working_directory>>
rectangle "Stage" as stage <<stage>>
rectangle "Repository" as repository <<repository>>
cloud "<$github_logo><$github_title>" as github
' <color:#000000></color>
' https://github.com/tupadr3/plantuml-icon-font-sprites
' https://plantuml.com/fr/sprite
' https://freeicons.io/artificial-intelligence,-ai-and-robotics-icons/github-icon-33249
' https://plantuml-editor.kkeisuke.com/
' plantuml -tsvg overview.pu
' rsvg-convert -f pdf -o overview.pdf overview.svg

note right of repository
  Local repository
end note

note right of github
  Remote repository
end note


working_directory =down=> stage: " git add"
stage =down=> repository: " git commit"
repository =down=> github: "git push "
github =up=> repository: " git pull"

@enduml
